<div class="row">
  <h2><%= @user.email %>'s Items</h2>
  <div class="col-md-8 center box">
    <table class="table table-hover">

      <tbody>
          <%= render partial: 'items/item', collection: @items %>
        <tr>
          <%= render partial: 'items/form', locals: { user: @user, item: @item } %>
        </tr>
      </tbody>
    </table>
  </div>
</div>
